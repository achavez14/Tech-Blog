profile.handlebars

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Profile</title>
</head>
<body>
    <h1>User Profile</h1>
    <p>Welcome, {{username}}!</p>
    <h2>Your Projects:</h2>
    <ul>
        {{#each projects}}
            <li>{{this.title}}</li>
        {{/each}}
    </ul>
    <a href="/logout">Logout</a>
</body>
</html>